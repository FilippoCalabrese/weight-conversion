{
    "name": "filippocalabrese/weight-conversion",
    "description": "Weight conversion in php",
    "keywords": [
        "FilippoCalabrese",
        "weight-conversion"
    ],
    "homepage": "https://github.com/filippocalabrese/weight-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Calabrese",
            "email": "calabresefilippo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "FilippoCalabrese\\WeightConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FilippoCalabrese\\WeightConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
